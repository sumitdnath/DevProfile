<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="110dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="90dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:scaleType="centerCrop"/>
    <TextView
            android:text="InjectHub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="18sp" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="28dp"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/imageView13"
            android:fontFamily="sans-serif-light" android:textColor="@color/colorPrimaryDark" android:textStyle="bold"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView19"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="0.86"
            app:layout_constraintEnd_toStartOf="@+id/imageView20"
            app:layout_constraintHorizontal_chainStyle="packed" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView13"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView20"
            app:layout_constraintStart_toEndOf="@+id/imageView19" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" app:layout_constraintTop_toTopOf="@+id/imageView19"
            app:layout_constraintBottom_toBottomOf="@+id/imageView19" android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView20" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="@+id/imageView20"
            app:layout_constraintBottom_toBottomOf="@+id/imageView20" app:layout_constraintVertical_bias="0.0"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline4"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView29" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline7" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/logo"
            app:layout_constraintDimensionRatio="w,1:1" android:scaleType="centerCrop"
            android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView30"
            app:layout_constraintStart_toEndOf="@+id/imageView29"
            android:layout_marginStart="24dp" app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29" android:scaleType="centerCrop"
    />
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView31"
            app:layout_constraintEnd_toStartOf="@+id/imageView33"
            app:layout_constraintBottom_toBottomOf="@+id/imageView33"
            app:layout_constraintTop_toTopOf="@+id/imageView33" app:layout_constraintStart_toStartOf="@+id/guideline4"
            app:layout_constraintDimensionRatio="1:1" android:scaleType="centerCrop" android:layout_marginEnd="24dp"
    />
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline7" app:layout_constraintGuide_begin="202dp"
                                          android:orientation="horizontal"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView33" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline7"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView28" app:layout_constraintDimensionRatio="w,1:1"
            android:scaleType="centerCrop" android:layout_marginEnd="16dp"/>
</android.support.constraint.ConstraintLayout>